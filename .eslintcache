}\r\n        {filteredExperiences.map(item => (\r\n          <PortfolioItem value={item} key={item.organization+item.title+item.timeframe}/>\r\n          ))}\r\n\r\n        {filteredMentorship.length > 0 && <h1 className=\"portf-section-header\">Mentorship</h1>}\r\n        {filteredMentorship.map(item => (\r\n          <PortfolioItem value={item} key={item.organization+item.title+item.timeframe}/>\r\n          ))}\r\n\r\n        {filteredAwards.length > 0 && <h1 className=\"portf-section-header\">Awards</h1>}\r\n        {filteredAwards.map(item => (\r\n          <PortfolioItem value={item} key={item.organization+item.title+item.timeframe}/>\r\n          ))}\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\n// Returns if item matches any of the filters in filter\r\nfunction matchesFilter(item, filters) {\r\n  for (let filterRaw of filters) {\r\n    let filter = filterRaw.toLowerCase();\r\n    if (item.organization.toLowerCase().includes(filter)\r\n        || item.title.toLowerCase().includes(filter)\r\n        || item.description.some(bullet => bullet.toLowerCase().includes(filter))\r\n        || item.tags.some(tag => tag.toLowerCase().includes(filter))) {\r\n          return true;\r\n    }\r\n  }\r\n  return false;\r\n}\r\n\r\nexport default Portfolio;",{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":6,"column":3,"nodeType":"54","messageId":"55","endLine":8,"endColumn":4},{"ruleId":"56","severity":1,"message":"57","line":41,"column":15,"nodeType":"58","endLine":41,"endColumn":82},{"ruleId":"52","severity":1,"message":"53","line":14,"column":3,"nodeType":"54","messageId":"55","endLine":16,"endColumn":4},{"ruleId":"48","replacedBy":"59"},{"ruleId":"50","replacedBy":"60"},{"ruleId":"52","severity":1,"message":"53","line":16,"column":3,"nodeType":"54","messageId":"55","endLine":18,"endColumn":4},"no-native-reassign",["61"],"no-negated-in-lhs",["62"],"no-useless-constructor","Useless constructor.","MethodDef